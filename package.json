{
  "name": "@prompticus/n8n-nodes-prompticus",
  "author": {
    "name": "ThumbsUpOrDown",
    "email": "thumbsupordown@promptic.us"
  },
  "description": "n8n node for Prompticus integration",
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "1.16.3",
    "n8n-workflow": "1.82.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://promptic.us",
  "keywords": [
    "n8n",
    "n8n-nodes",
    "prompticus"
  ],
  "license": "MIT",
  "main": "index.js",
  "n8n": {
    "credentials": [
      "src/credentials/PrompticusApi.credentials.js"
    ],
    "n8nNodesApiVersion": 1,
    "nodes": [
      "src/nodes/Prompticus/Prompticus.node.js",
      "src/nodes/Prompticus/prompticus.svg"
    ]
  },
  "packageManager": "pnpm@9.1.0",
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/prompticus/n8n-nodes-prompticus"
  },
  "scripts": {
    "build": "tsc && cp src/nodes/Prompticus/prompticus.svg dist/src/nodes/Prompticus/",
    "dev": "tsc --watch",
    "dev:clean": "rm -rf node_modules/ && rm -rf dist/",
    "dev:link": "npm run build && npm link",
    "dev:n8n": "cd ~/.n8n/custom/ && npm link n8n-nodes-prompticus && n8n",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint package.json src/**/*.ts"
  },
  "version": "1.0.0"
}
